<%- include('partial/header') %>
<%- include('partial/navbar') %>
<table class="table">

    <tr>
        <th>id</th>
        <th>First_name</th>
        <th>Last_name</th>
        <th>Email</th>
        <th>Gender</th>
        <th>Action</th>
    </tr>
    <% data.forEach(list => { %>
    <tr>
        <td><%= list.id%></td>
        <td><%= list.first_name%></td>
        <td><%= list.last_name%></td>
        <td><%= list.email%></td>
        <td><%= list.gender%></td>
        <td>Edit | Delete</td>
    </tr>
    <% }) %>


</table>
<%- include('partial/footer') %>